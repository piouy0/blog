{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    [
      "module-resolver",
      {
        "extensions": ["*", ".ts", ".js", ".tsx", ".jsx"],
        "root": ["./src"],
        "alias": {}
      }
    ]
  ]
}
